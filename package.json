{
  "name": "kitsune",
  "version": "1.0.0",
  "main": "dist/index.js",
  "author": "Bruno Sartori <brunosartori.dev@gmail.com>",
  "description": "An API for handling my (not smart) devices on Google Home",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "dotenvx run -- nodemon",
    "build": "tsc",
    "serve": "node dist/index.js",
    "db:up": "docker-compose up -d",
    "db:create": "sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:clean": "sudo docker exec -it kitsune-db bash -c \"mysql -u root -pbukassas9 -e 'use nekomata; delete from Files;'\"",
    "db:shell": "sudo docker exec -it kitsune-db bash",
    "cargo:test": "cargo test",
    "cargo-build": "cargo build --message-format=json > cargo.log",
    "cross-build": "cross build --message-format=json > cross.log",
    "postcargo-build": "neon dist < cargo.log",
    "postcross-build": "neon dist -m /target < cross.log",
    "cargo:debug": "npm run cargo-build --",
    "cargo:build": "npm run cargo-build -- --release",
    "postcargo:build": "mv index.node lib.node",
    "cargo:cross": "npm run cross-build -- --release",
    "build:cargo": "cargo-cp-artifact -nc lib.node -- cargo build --message-format=json-render-diagnostics",
    "docker:ngrok": "docker run --net=host -it -e NGROK_AUTHTOKEN=2nDBam1NSanf2dqKVTQMCTSY9Y4_6f2TH8oC3TeBP43SmjdaP ngrok/ngrok:latest http 3000"
  },
  "devDependencies": {
    "@neon-rs/cli": "0.1.73",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "cargo-cp-artifact": "^0.1.9",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.5",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.14.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross": "^1.0.0",
    "express": "^4.21.0",
    "express-session": "^1.18.1",
    "http-status": "^1.7.4",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "sequelize": "^6.37.3",
    "uuid": "^10.0.0"
  },
  "_moduleAliases": {
    "@interfaces": "dist/interfaces",
    "@config": "dist/config",
    "@utils": "dist/utils",
    "@errors": "dist/errors",
    "@controllers": "dist/controllers",
    "@middlewares": "dist/middlewares",
    "@models": "dist/database/models",
    "@routes": "dist/routes",
    "@services": "dist/services"
  }
}
